{
  "name": "paycore/openfintech-data",
  "license": "MIT",
  "type": "project",
  "description": "Openfintech data",
  "require": {
    "php": "^7.0 || ^8.0",
    "justinrainbow/json-schema": "^5.2",
    "ext-json": "*"
  }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Oft\\Provider\\": "./",
      "Oft\\Generator\\": "./etc/doc-build/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests/"
    }
  },
  "prefer-stable": true,
  "config": {
      "bin-dir": "bin"
  },
  "suggest": {
    "paycore/openfintech-meta": "Provides additional information about images, etc."
  },
    "extra": {
       "branch-alias": {
         "dev-master": "0.1.x-dev",
         "dev-dev": "0.2.x-dev"
       }
   }
}
